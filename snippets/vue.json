{
  "setup": {
    "prefix": "tst",
    "body": [
      "<script lang=\"ts\" setup>",
      "\t$1",
      "</script>",
      "",
      "<template lang=\"pug\">",
      "\t$2",
      "</template>",
      "",
      "<style lang=\"scss\">",
      "\t$3",
      "</style>"
    ],
    "description": "Vue3 ts base template"
  },
  "defineProps": {
    "prefix": "defp",
    "body": ["const props = defineProps<{$1}>()"]
  },
  "defineEmits": {
    "prefix": "defe",
    "body": ["const emits = defineEmits<{$1}>()"]
  }
}
